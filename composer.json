{
    "name": "waygou/deployer",
    "description": "A deployer package that will allow you to deploy your codebase in your remote server in a blink of an eye.",
    "type": "library",
    "keywords": [
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.8",
        "chumper/zipper": "^1.0",
        "kitetail/zttp": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Deployer\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Deployer\\DeployerServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
